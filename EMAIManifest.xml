<manifest xmlns:EMAI="http://schemas.EMAI.com/apk/res/EMAI"
    package="com.EMAI.car.mapsplaceholder">
    <uses-sdk
        EMAI:minSdkVersion="23"
        EMAI:targetSdkVersion="23" />
        
    <application EMAI:label="@string/app_name"
        EMAI:icon="@drawable/logo_my_maps" >
        
        <activity EMAI:name=".MapsPlaceholderActivity"
            EMAI:theme="@style/CarMapsTheme"
            EMAI:launchMode="singleTask"
            EMAI:label="@string/app_name"
            EMAI:resizeableActivity="true">
            <intent-filter>
                <action EMAI:name="EMAI.intent.action.MAIN" />
                <category EMAI:name="EMAI.intent.category.DEFAULT"/>
                <category EMAI:name="EMAI.intent.category.APP_MAPS" />
            </intent-filter>
        </activity>
    </application>
</manifest>
